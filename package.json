{
	"name": "tbeseda-com",
	"version": "2.0.0",
	"description": "Taylor's homepage",
	"scripts": {
		"deploy": "npx arc deploy -v",
		"deploy.production": "npx arc deploy --production -v",
		"deploy.both": "npm run deploy && npm run deploy.production",
		"rome": "npm run rome.fix && npx rome ci .",
		"rome.fix": "npm run rome.check.apply && npm run rome.format.write",
		"rome.check": "npx rome check .",
		"rome.check.apply": "npx rome check . --apply",
		"rome.format": "npx rome format .",
		"rome.format.write": "npx rome format . --write",
		"start": "npx arc sandbox",
		"tape": "npx tape ./test/**/*.mjs | tap-arc",
		"test": "npm run rome && npm run tape"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/tbeseda/tbeseda-com.git"
	},
	"keywords": [
		"homepage",
		"enhance"
	],
	"author": "tbeseda",
	"license": "ISC",
	"homepage": "https://tbeseda.com",
	"dependencies": {
		"@architect/functions": "^6.0.1",
		"@aws-sdk/client-s3": "^3.369.0",
		"@enhance/arc-plugin-enhance": "^6.2.0",
		"@tiptap/core": "^2.0.3",
		"@tiptap/extension-highlight": "^2.0.3",
		"@tiptap/extension-image": "^2.0.3",
		"@tiptap/extension-link": "^2.0.3",
		"@tiptap/extension-superscript": "^2.0.3",
		"@tiptap/extension-table": "^2.0.3",
		"@tiptap/extension-table-cell": "^2.0.3",
		"@tiptap/extension-table-header": "^2.0.3",
		"@tiptap/extension-table-row": "^2.0.3",
		"@tiptap/pm": "^2.0.3",
		"@tiptap/starter-kit": "^2.0.3",
		"@vrite/sdk": "^2023.6.30-beta",
		"chalk": "^5.3.0",
		"feed": "^4.2.2",
		"lambda-multipart-parser": "^1.0.1",
		"pm2html": "^0.1.10",
		"rss-to-json": "^2.1.1",
		"xterm": "^5.2.1",
		"xterm-readline": "^1.1.1"
	},
	"devDependencies": {
		"@architect/architect": "10.13.1",
		"@architect/plugin-lambda-invoker": "^1.0.0",
		"@architect/sandbox": "5.7.2",
		"@enhance/types": "^0.6.0",
		"arcdown": "^2.2.0",
		"microformats-parser": "^1.4.1",
		"rome": "^12.1.3",
		"tap-arc": "^0.3.5",
		"tape": "^5.6.5"
	}
}
