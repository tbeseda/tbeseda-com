{
  "name": "tbeseda-com",
  "version": "2.0.0",
  "description": "Taylor's homepage",
  "scripts": {
    "deploy.both": "npm run deploy.staging && npm run deploy.production",
    "deploy.production": "arc deploy --production",
    "deploy.staging": "arc deploy",
    "lint": "eslint --ext .mjs --ext .js .",
    "lint.fix": "npm run lint -- --fix",
    "start": "arc sandbox",
    "test": "npm run lint && node --test test/",
    "check-updates": "npx npm-check-updates"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tbeseda/tbeseda-com.git"
  },
  "keywords": [
    "homepage",
    "enhance"
  ],
  "author": "tbeseda",
  "license": "ISC",
  "homepage": "https://tbeseda.com",
  "dependencies": {
    "@architect/asap": "^7.0.7",
    "@architect/functions": "^8.0.4",
    "@aws-lite/client": "^0.17.2",
    "@aws-lite/dynamodb": "^0.3.4",
    "@aws-lite/s3": "^0.1.18",
    "@aws-sdk/client-s3": "^3.515.0",
    "@enhance/app-core": "https://gitpkg.now.sh/tbeseda/fork-arc-plugin-enhance/app-core?4ada042",
    "@enhance/app-loader": "https://gitpkg.now.sh/tbeseda/fork-arc-plugin-enhance/app-loader?4ada042",
    "@enhance/arc-plugin-enhance": "https://gitpkg.now.sh/tbeseda/fork-arc-plugin-enhance?4ada042",
    "@enhance/enhance-style-transform": "^0.1.2",
    "@enhance/import-transform": "^4.0.0",
    "@milkdown/core": "^7.3.5",
    "@milkdown/plugin-listener": "^7.3.5",
    "@milkdown/preset-commonmark": "^7.3.5",
    "@tiptap/core": "^2.2.3",
    "@tiptap/extension-highlight": "^2.2.3",
    "@tiptap/extension-image": "^2.2.3",
    "@tiptap/extension-link": "^2.2.3",
    "@tiptap/extension-superscript": "^2.2.3",
    "@tiptap/extension-table": "^2.2.3",
    "@tiptap/extension-table-cell": "^2.2.3",
    "@tiptap/extension-table-header": "^2.2.3",
    "@tiptap/extension-table-row": "^2.2.3",
    "@tiptap/pm": "^2.2.3",
    "@tiptap/starter-kit": "^2.2.3",
    "@vrite/sdk": "^0.4.0",
    "@zachleat/snow-fall": "^1.0.2",
    "busboy": "^1.6.0",
    "chalk": "^5.3.0",
    "enhance-app-core": "file:../enhance-app",
    "feed": "^4.2.2",
    "header-timers": "^0.2.0",
    "minisearch": "^6.3.0",
    "pm2html": "^0.1.12",
    "pug": "^3.0.2",
    "server-timings-elem": "^1.1.0",
    "waylon": "^0.3.1",
    "xterm": "^5.3.0",
    "xterm-readline": "^1.1.1"
  },
  "devDependencies": {
    "@architect/architect": "^11.0.5",
    "@architect/plugin-go": "^0.0.2",
    "@architect/plugin-lambda-invoker": "^1.2.0",
    "@architect/plugin-rust": "^0.1.1",
    "@aws-lite/dynamodb-types": "^0.3.6",
    "@enhance/types": "^0.7.0",
    "eslint": "^8.56.0",
    "eslint-config-standard": "^17.1.0",
    "microformats-parser": "^2.0.2"
  },
  "eslintConfig": {
    "extends": "standard",
    "ignorePatterns": [
      "src/vendor",
      "public/browser",
      "public/bundles"
    ],
    "rules": {
      "space-unary-ops": 0,
      "comma-dangle": [
        "error",
        "always-multiline"
      ]
    }
  }
}
