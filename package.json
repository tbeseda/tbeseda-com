{
  "name": "tbeseda-com",
  "version": "2.0.0",
  "description": "Taylor's homepage",
  "scripts": {
    "postinstall": "vendurl",
    "create-styles": "enhance-styles --config=./enhance-styles.json --output=./public/css/enhance-styles.css",
    "deploy.both": "npm run deploy.staging && npm run deploy.production",
    "deploy.production": "arc deploy --production",
    "deploy.staging": "arc deploy",
    "biome": "biome check --apply ./",
    "start": "arc sandbox",
    "test": "biome ci ./ && node --test test/",
    "check-updates": "npx npm-check-updates"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tbeseda/tbeseda-com.git"
  },
  "keywords": [
    "homepage",
    "enhance"
  ],
  "author": "tbeseda",
  "license": "ISC",
  "homepage": "https://tbeseda.com",
  "dependencies": {
    "@architect/asap": "^7.0.10",
    "@architect/functions": "^8.1.6",
    "@aws-lite/client": "^0.21.7",
    "@aws-lite/dynamodb": "^0.3.4",
    "@aws-lite/s3": "^0.1.21",
    "@aws-sdk/client-s3": "^3.583.0",
    "@enhance/core": "0.1.0-alpha.0",
    "@enhance/enhance-style-transform": "^0.1.2",
    "@enhance/import-transform": "^4.0.1",
    "@enhance/loader": "0.1.0-alpha.0",
    "@milkdown/core": "^7.3.6",
    "@milkdown/plugin-listener": "^7.3.6",
    "@milkdown/preset-commonmark": "^7.3.6",
    "@resvg/resvg-wasm": "^2.6.2",
    "@tiptap/core": "^2.4.0",
    "@tiptap/extension-highlight": "^2.4.0",
    "@tiptap/extension-image": "^2.4.0",
    "@tiptap/extension-link": "^2.4.0",
    "@tiptap/extension-superscript": "^2.4.0",
    "@tiptap/extension-table": "^2.4.0",
    "@tiptap/extension-table-cell": "^2.4.0",
    "@tiptap/extension-table-header": "^2.4.0",
    "@tiptap/extension-table-row": "^2.4.0",
    "@tiptap/pm": "^2.4.0",
    "@tiptap/starter-kit": "^2.4.0",
    "@vrite/sdk": "^0.4.6",
    "@zachleat/snow-fall": "^1.0.2",
    "busboy": "^1.6.0",
    "chalk": "^5.3.0",
    "feed": "^4.2.2",
    "header-timers": "^0.2.1",
    "highlight.js": "^11.9.0",
    "html-skelly": "^0.1.0",
    "i": "^0.3.7",
    "lz-string": "^1.5.0",
    "minisearch": "^6.3.0",
    "pm2html": "^0.1.12",
    "pug": "^3.0.2",
    "quill": "^2.0.2",
    "satori": "^0.10.13",
    "server-timings-elem": "^1.1.0",
    "waylon": "^0.3.1",
    "xterm": "^5.3.0",
    "xterm-readline": "^1.1.1"
  },
  "devDependencies": {
    "@architect/architect": "^11.0.12",
    "@architect/plugin-bundles": "^3.2.0",
    "@architect/plugin-go": "^0.0.2",
    "@architect/plugin-lambda-invoker": "^2.0.0",
    "@architect/plugin-rust": "^0.1.1",
    "@aws-lite/dynamodb-types": "^0.3.6",
    "@begin/deploy": "^0.22.4",
    "@biomejs/biome": "^1.7.3",
    "@enhance/styles": "^6.5.0",
    "@enhance/types": "^0.7.0",
    "microformats-parser": "^2.0.2",
    "vendurl": "^0.3.0"
  },
  "vendurl": {
    "packages": {
      "monospace.css": {
        "specifier": "https://raw.githubusercontent.com/800147/monospace/main/src/monospace.css",
        "destination": "./public/css"
      }
    }
  }
}
