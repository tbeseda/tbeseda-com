{
  "name": "tbeseda-com",
  "version": "2.0.0",
  "description": "Taylor's homepage",
  "scripts": {
    "deploy.both": "npm run deploy.staging && npm run deploy.production",
    "deploy.production": "arc deploy --production",
    "deploy.staging": "arc deploy",
    "check": "biome check --apply ./",
    "lint": "biome lint --apply ./",
    "format": "biome format --write ./",
    "start": "arc sandbox",
    "test": "npm run check && node --test test/",
    "check-updates": "npx npm-check-updates"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tbeseda/tbeseda-com.git"
  },
  "keywords": ["homepage", "enhance"],
  "author": "tbeseda",
  "license": "ISC",
  "homepage": "https://tbeseda.com",
  "dependencies": {
    "@architect/asap": "^7.0.7",
    "@architect/functions": "^8.0.4",
    "@aws-lite/client": "^0.19.0",
    "@aws-lite/dynamodb": "^0.3.4",
    "@enhance/core": "^0.1.0-alpha.0",
    "@enhance/enhance-style-transform": "^0.1.2",
    "@enhance/loader": "^0.1.0-alpha.0",
    "@aws-lite/s3": "^0.1.20",
    "@aws-sdk/client-s3": "^3.529.1",
    "@enhance/import-transform": "^4.0.1",
    "@milkdown/core": "^7.3.5",
    "@milkdown/plugin-listener": "^7.3.5",
    "@milkdown/preset-commonmark": "^7.3.5",
    "@tiptap/core": "^2.2.4",
    "@tiptap/extension-highlight": "^2.2.4",
    "@tiptap/extension-image": "^2.2.4",
    "@tiptap/extension-link": "^2.2.4",
    "@tiptap/extension-superscript": "^2.2.4",
    "@tiptap/extension-table": "^2.2.4",
    "@tiptap/extension-table-cell": "^2.2.4",
    "@tiptap/extension-table-header": "^2.2.4",
    "@tiptap/extension-table-row": "^2.2.4",
    "@tiptap/pm": "^2.2.4",
    "@tiptap/starter-kit": "^2.2.4",
    "@vrite/sdk": "^0.4.1",
    "@zachleat/snow-fall": "^1.0.2",
    "busboy": "^1.6.0",
    "chalk": "^5.3.0",
    "feed": "^4.2.2",
    "header-timers": "^0.2.1",
    "html-skelly": "^0.1.0",
    "minisearch": "^6.3.0",
    "pm2html": "^0.1.12",
    "pug": "^3.0.2",
    "server-timings-elem": "^1.1.0",
    "waylon": "^0.3.1",
    "xterm": "^5.3.0",
    "xterm-readline": "^1.1.1"
  },
  "devDependencies": {
    "@architect/architect": "^11.0.5",
    "@architect/plugin-bundles": "^3.2.0",
    "@architect/plugin-go": "^0.0.2",
    "@architect/plugin-lambda-invoker": "^1.2.0",
    "@architect/plugin-rust": "^0.1.1",
    "@aws-lite/dynamodb-types": "^0.3.6",
    "@biomejs/biome": "^1.6.0",
    "@enhance/types": "^0.7.0",
    "microformats-parser": "^2.0.2"
  },
  "eslintConfig": {
    "extends": "standard",
    "ignorePatterns": ["src/vendor", "public/browser", "public/bundles"],
    "rules": {
      "space-unary-ops": 0,
      "comma-dangle": ["error", "always-multiline"]
    }
  }
}
