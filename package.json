{
  "name": "tbeseda-com",
  "version": "2.0.0",
  "description": "Taylor's homepage",
  "scripts": {
    "deploy.both": "npm run deploy && npm run deploy.production",
    "deploy.production": "arc deploy --production",
    "deploy": "arc deploy",
    "lint": "eslint --ext .mjs --ext .js .",
    "lint.fix": "npm run lint -- --fix",
    "start": "arc sandbox",
    "tape": "tape ./test/**/*.mjs | tap-arc",
    "test": "npm run lint && npm run tape"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tbeseda/tbeseda-com.git"
  },
  "keywords": [
    "homepage",
    "enhance"
  ],
  "author": "tbeseda",
  "license": "ISC",
  "homepage": "https://tbeseda.com",
  "dependencies": {
    "@architect/functions": "^7.0.0",
    "@aws-sdk/client-s3": "^3.451.0",
    "@enhance/arc-plugin-enhance": "^9.1.2",
    "@enhance/enhance-plugin-performance-budget": "^0.1.0",
    "@enhance/form-elements": "^1.1.0",
    "@tiptap/core": "^2.1.12",
    "@tiptap/extension-highlight": "^2.1.12",
    "@tiptap/extension-image": "^2.1.12",
    "@tiptap/extension-link": "^2.1.12",
    "@tiptap/extension-superscript": "^2.1.12",
    "@tiptap/extension-table": "^2.1.12",
    "@tiptap/extension-table-cell": "^2.1.12",
    "@tiptap/extension-table-header": "^2.1.12",
    "@tiptap/extension-table-row": "^2.1.12",
    "@tiptap/pm": "^2.1.12",
    "@tiptap/starter-kit": "^2.1.12",
    "@vrite/sdk": "^2023.6.30-beta",
    "busboy": "^1.6.0",
    "chalk": "^5.3.0",
    "feed": "^4.2.2",
    "header-timers": "^0.2.0",
    "pm2html": "^0.1.12",
    "pug": "^3.0.2",
    "rss-to-json": "^2.1.1",
    "waylon": "^0.3.1",
    "xterm": "^5.3.0",
    "xterm-readline": "^1.1.1"
  },
  "devDependencies": {
    "@architect/architect": "^10.16.1",
    "@architect/plugin-go": "^0.0.2",
    "@architect/plugin-lambda-invoker": "^1.2.0",
    "@architect/plugin-rust": "^0.1.1",
    "@enhance/types": "^0.6.1",
    "arcdown": "^2.2.1",
    "eslint": "^8.53.0",
    "eslint-config-standard": "^17.1.0",
    "lambda-runtimes": "^1.1.6",
    "microformats-parser": "^2.0.2",
    "tap-arc": "^1.2.2",
    "tape": "^5.7.2"
  },
  "eslintConfig": {
    "extends": "standard",
    "ignorePatterns": [
      "src/vendor",
      "public/browser",
      "public/bundles"
    ],
    "rules": {
      "space-unary-ops": 0,
      "comma-dangle": [
        "error",
        "always-multiline"
      ]
    }
  }
}
